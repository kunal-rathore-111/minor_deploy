{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "functions": {
        "src/index.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "env": {
        "NODE_ENV": "production"
    }
}